{
    "version": 2,
    "extra_config": {
      "github_com/devopsfaith/krakend-cors": {
        "allow_origins": [
          "http://me.filebox.com:3000/",
          "http://127.0.0.1:8081",
          "http://127.0.0.1:8082"
        ],
        "expose_headers": [
          "Content-Length"
        ],
        "max_age": "12h",
        "allow_methods": [
          "GET",
          "POST",
          "PUT",
          "OPTIONS",
          "PATCH"
        ],
        "allow_headers": [
          "Origin",
          "Content-Type",
          "Authorization",
          "X-Requested-With"
        ],
        "allow_origin_func": [
          "http://me.filebox.com:3000",
          "http://localhost:3000"
        ],
        "allow_credentials": true
      }
    },
    "timeout": "3000ms",
    "output_encoding": "json",
    "name": "Filebox",
    "port": 9091,
    "read_timeout": "10s",
    "write_timeout": "10s",
    "idle_timeout": "10s",
    "read_header_timeout": "10s",
    "endpoints": [
      {
        "endpoint": "/auth",
        "method": "GET",
        "output_encoding": "json",
        "concurrent_calls": 1,
        "querystring_params": [],
        "headers_to_pass": [
          "*"
        ],
        "backend": [
          {
            "url_pattern": "/ServiceLogin",
            "encoding": "json",
            "extra_config": {
              "github.com/devopsfaith/krakend-martian": {
                "fifo.Group": {
                  "scope": [
                    "request",
                    "response"
                  ],
                  "aggregateErrors": true,
                  "modifiers": [
                    {
                      "header.JwtClaim": {
                        "scope": [
                          "request"
                        ],
                        "claim": "sub",
                        "header": "UserID"
                      }
                    }
                  ]
                }
              }
            },
            "host": [
              "http://127.0.0.1:8081"
            ],
            "disable_host_sanitize": false,
            "method": "GET"
          },
          {
            "url_pattern": "/createUser",
            "encoding": "json",
            "extra_config": {
              "github.com/devopsfaith/krakend-martian": {
                "fifo.Group": {
                  "scope": [
                    "request",
                    "response"
                  ],
                  "aggregateErrors": true,
                  "modifiers": [
                    {
                      "header.JwtClaim": {
                        "scope": [
                          "request"
                        ],
                        "claim": "sub",
                        "header": "UserID"
                      }
                    }
                  ]
                }
              }
            },
            "host": [
              "http://127.0.0.1:8082"
            ],
            "disable_host_sanitize": false,
            "method": "GET"
          }
        ],
        "extra_config": {
          "github.com/devopsfaith/krakend-jose/validator": {
            "alg": "HS256",
            "jwk-url": "http://127.0.0.1:8081/keys",
            "cookie_key": "",
            "audience": [
              "http://api.example.com"
            ],
            "roles": [
              "role_a",
              "role_b"
            ],
            "disable_jwk_security": true,
            "roles_key": "roles",
            "issuer": "https://krakend.io"
          }
        }
      },
      {
        "endpoint": "/token",
        "headers_to_pass": [
          "*"
        ],
        "backend": [
          {
            "url_pattern": "/token",
            "encoding": "json",
            "host": [
              "http://127.0.0.1:8081"
            ],
            "extra_config": {}
          }
        ],
        "extra_config": {
          "github.com/devopsfaith/krakend-jose/signer": {
            "alg": "HS256",
            "keys-to-sign": [
              "access_token",
              "refresh_token"
            ],
            "jwk-url": "http://127.0.0.1:8081/keys",
            "disable_jwk_security": true,
            "kid": "sim2"
          }
        },
        "output_encoding": "json",
        "concurrent_calls": 1
      },
      {
        "endpoint": "/create-folder",
        "method": "POST",
        "headers_to_pass": [
          "*"
        ],
        "extra_config": {
          "github.com/devopsfaith/krakend-jose/validator": {
            "alg": "HS256",
            "jwk-url": "http://127.0.0.1:8081/keys",
            "cookie_key": "",
            "audience": [
              "http://api.example.com"
            ],
            "roles": [
              "role_a",
              "role_b"
            ],
            "disable_jwk_security": true,
            "roles_key": "roles",
            "issuer": "https://krakend.io"
          }
        },
        "output_encoding": "json",
        "concurrent_calls": 1,
        "backend": [
          {
            "url_pattern": "/createFolder",
            "encoding": "json",
            "extra_config": {
              "github.com/devopsfaith/krakend-martian": {
                "fifo.Group": {
                  "scope": [
                    "request",
                    "response"
                  ],
                  "aggregateErrors": true,
                  "modifiers": [
                    {
                      "header.JwtClaim": {
                        "scope": [
                          "request"
                        ],
                        "claim": "sub",
                        "header": "UserID"
                      }
                    }
                  ]
                }
              }
            },
            "method": "POST",
            "host": [
              "http://127.0.0.1:8082"
            ],
            "disable_host_sanitize": false
          }
        ]
      },
      {
        "endpoint": "/ls",
        "method": "GET",
        "extra_config": {
          "github.com/devopsfaith/krakend-jose/validator": {
            "alg": "HS256",
            "jwk-url": "http://127.0.0.1:8081/keys",
            "cookie_key": "",
            "audience": [
              "http://api.example.com"
            ],
            "roles": [
              "role_a",
              "role_b"
            ],
            "disable_jwk_security": true,
            "roles_key": "roles",
            "issuer": "https://krakend.io"
          },
          "github.com/devopsfaith/krakend-martian": {
            "fifo.Group": {
              "scope": [
                "request",
                "response"
              ],
              "aggregateErrors": true,
              "modifiers": [
                {
                  "header.JwtClaim": {
                    "scope": [
                      "request"
                    ],
                    "claim": "sub",
                    "header": "UserID"
                  }
                }
              ]
            }
          }
        },
        "output_encoding": "json",
        "concurrent_calls": 1,
        "querystring_params": [
          "path"
        ],
        "backend": [
          {
            "url_pattern": "/ls",
            "encoding": "json",
            "extra_config": {
              "github.com/devopsfaith/krakend/proxy": {
                "flatmap_filter": [
                  {
                    "type": "del",
                    "args": [
                      "files.*.userId"
                    ]
                  }
                ]
              }
            },
            "method": "GET",
            "host": [
              "http://127.0.0.1:8082"
            ],
            "disable_host_sanitize": false
          }
        ],
        "headers_to_pass": [
          "Authorization"
        ]
      }
    ],
    "cache_ttl": "10ms"
  }